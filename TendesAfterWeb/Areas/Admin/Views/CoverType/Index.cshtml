@model IEnumerable<TendesAfter.Models.CoverType>
@{
    ViewData["Title"] = "Index";
}
<partial name="_Notification"/>
<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">CoverType List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="CoverType" asp-action="Create" class="btn btn-primary">
                <i class="bi bi-plus-square"></i> &nbsp; Create new CoverType
            </a>
        </div>
    </div>
    <br /><br />
             

<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
              CoverType Name
            </th>
 
            <th>
                Edit
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.Name
                </td>
                <td>
                    <div class="w-75 btn-group" role="group">
                            <a asp-controller="CoverType" asp-action="Edit" asp-route-id="@obj.Id" class="btn btn-primary mx-2"><i class="bi bi-pen"></i>Edit</a>
                          <a asp-controller="CoverType" asp-action="Delete" asp-route-id="@obj.Id"  class= "btn btn-danger mx-2"> <i class="bi bi-trash3"></i> Delete</a>
                    </div>
                </td>
            </tr>
        }
    </tbody>
</table>
</div>

